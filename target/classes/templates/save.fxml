<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="540.0" prefWidth="837.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.SaveController">
    <TabPane prefHeight="537.0" prefWidth="837.0" tabClosingPolicy="UNAVAILABLE">
        <Tab text="Атракция">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <Label layoutX="262.0" layoutY="20.0" text="Запиши атракция:">
                    <font>
                        <Font name="Comic Sans MS" size="36.0"/>
                    </font>
                </Label>
                <Label layoutX="140.0" layoutY="137.0" text="Име:"/>
                <Label layoutX="140.0" layoutY="204.0" text="Минимална възраст:"/>
                <TextField fx:id="attractionName" layoutX="441.0" layoutY="131.0"/>
                <TextField fx:id="minAge" layoutX="441.0" layoutY="198.0"/>
                <Button fx:id="saveAttraction" layoutX="340.0" layoutY="304.0" mnemonicParsing="false"
                        text="Запиши"/>
                <Button fx:id="backAttraction" layoutX="14.0" layoutY="439.0" mnemonicParsing="false"
                        text="Назад"/>
            </AnchorPane>
        </Tab>
        <Tab text="Мениджър">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <Label layoutX="256.0" layoutY="39.0" text="Запиши мениджър">
                    <font>
                        <Font name="Comic Sans MS" size="36.0"/>
                    </font>
                </Label>
                <Label layoutX="139.0" layoutY="146.0" text="Име:"/>
                <Label layoutX="139.0" layoutY="217.0" text="Заплата:"/>
                <TextField fx:id="nameManager" layoutX="307.0" layoutY="140.0"/>
                <TextField fx:id="salary" layoutX="307.0" layoutY="211.0"/>
                <Button fx:id="saveManager" layoutX="330.0" layoutY="310.0" mnemonicParsing="false"
                        text="Запиши"/>
                <Button fx:id="backManager" layoutX="14.0" layoutY="439.0" mnemonicParsing="false"
                        text="Назад"/>
            </AnchorPane>
        </Tab>
        <Tab text="Парк" fx:id="parkTab">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <Label layoutX="305.0" layoutY="34.0" text="Запиши парк">
                    <font>
                        <Font name="Comic Sans MS" size="36.0"/>
                    </font>
                </Label>
                <Label layoutX="91.0" layoutY="149.0" text="Име:"/>
                <Label layoutX="91.0" layoutY="208.0" text="Цена на билет:"/>
                <Label layoutX="91.0" layoutY="273.0" text="Мениджър на парка:"/>
                <Label layoutX="91.0" layoutY="336.0" text="Атракции:"/>
                <TextField fx:id="parkName" layoutX="370.0" layoutY="137.0"/>
                <TextField fx:id="price" layoutX="370.0" layoutY="196.0"/>
                <ComboBox fx:id="pm" layoutX="370.0" layoutY="330.0" prefHeight="39.0" prefWidth="224.0"
                          promptText="Избери мениджър"/>
                <Button fx:id="savePark" layoutX="374.0" layoutY="412.0" mnemonicParsing="false"
                        text="Запиши"/>
                <MenuButton fx:id="parkAttractions" layoutX="370.0" layoutY="330.0" prefHeight="39.0"
                            prefWidth="224.0" text="Избери атракции"/>
                <Button fx:id="backPark" layoutX="14.0" layoutY="439.0" mnemonicParsing="false"
                        text="Назад"/>
                <ComboBox fx:id="parkManager" layoutX="370.0" layoutY="267.0" prefHeight="39.0"
                          prefWidth="225.0" promptText="Избери мениджър"/>
            </AnchorPane>
        </Tab>
        <Tab text="Дете" fx:id="childTab">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="455.0" prefWidth="837.0">
                <Label layoutX="73.0" layoutY="136.0" prefHeight="32.0" prefWidth="51.0" text="Име:"/>
                <Label layoutX="73.0" layoutY="209.0" text="Възраст:"/>
                <Label layoutX="73.0" layoutY="273.0" text="Атракции:"/>
                <TextField fx:id="childName" layoutX="259.0" layoutY="133.0"/>
                <TextField fx:id="age" layoutX="259.0" layoutY="203.0"/>
                <Label layoutX="76.0" layoutY="334.0" text="Парк:"/>
                <Label layoutX="253.0" layoutY="31.0" text="Запиши дете:">
                    <font>
                        <Font name="Comic Sans MS" size="36.0"/>
                    </font>
                </Label>
                <Button fx:id="saveChild" layoutX="327.0" layoutY="399.0" mnemonicParsing="false"
                        text="Запиши"/>
                <MenuButton fx:id="childAttractions" layoutX="259.0" layoutY="267.0" prefHeight="39.0"
                            prefWidth="223.0" text="Избери атракции"/>
                <ComboBox fx:id="childParks" layoutX="259.0" layoutY="334.0" prefHeight="39.0"
                          prefWidth="224.0" promptText="Избери парк"/>
                <Button fx:id="backChild" layoutX="14.0" layoutY="438.0" mnemonicParsing="false"
                        text="Назад"/>
            </AnchorPane>
        </Tab>
    </TabPane>
</AnchorPane>
